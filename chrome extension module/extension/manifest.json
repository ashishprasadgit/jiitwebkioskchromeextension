{
"name":"Webkiosk",
"description":"Know your attendance without logging into JIIT Webkiosk",
"version":"0.1.1",
"manifest_version":2,
"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
"homepage_url": "http://jiitsimplified.com",
"browser_action": {
        "default_icon": "logo.png",
        "default_popup": "background.html"
    },
"icons":
	{ "16": "logo.png",
	   "48": "logo.png",
	  "128": "logo.png" 
	},
"permissions":["http://*/*","webRequest","storage","browsingData"],
"background": {"scripts": ["options.js"] }
}